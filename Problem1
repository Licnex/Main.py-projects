def get_multiples(num1, num2, RangeTop):
    """checks for multiples of number in range(1-RangeTop)"""
    multiples = []
    i=1
    for i in range(1,RangeTop):
        if i % num1 == 0 and i != 0:
                multiples.append(i)
        if i % num2 == 0 and i != 0:
                multiples.append(i)
    return multiples
print(get_multiples(3,5,1000))